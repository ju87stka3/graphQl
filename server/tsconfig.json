{
  "compilerOptions": {
    "lib": [
      "es2017",
      "esnext",
      "esnext.asynciterable"
    ],
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
